<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<tongue>
	<race>silverinc_feronwarrior</race>

	<coveringType>TONGUE</coveringType>
	
	<defaultLength>3</defaultLength>
	
	<name><![CDATA[tongue]]></name>
	<namePlural><![CDATA[tongues]]></namePlural>
	
	<descriptorsMasculine>
		<descriptor><![CDATA[soft]]></descriptor>
	</descriptorsMasculine>
	<!-- Same as above, but for feminine characters. -->
	<descriptorsFeminine>
		<descriptor><![CDATA[soft]]></descriptor>
	</descriptorsFeminine>
	
	<bodyDescription><![CDATA[
		[npc.Her] mouth holds [npc.a_tongueLength], [npc.tongueColour(true)], feron [npc.tongue]#IF(npc.isPiercedTongue()), which has been pierced#ENDIF.
	]]></bodyDescription>
	
	<defaultTongueModifiers>
		<modifier>WIDE</modifier>
	</defaultTongueModifiers>
	
</tongue>
